{
  "name": "zona21",
  "version": "0.4.9",
  "description": "Plataforma de ingestão, catalogação e seleção de mídia para foto e vídeo",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder --config electron-builder.yml",
    "preview": "vite preview",
    "electron:dev": "electron-rebuild -f && NODE_OPTIONS=--max-old-space-size=4096 vite --port 5174 --strictPort",
    "electron:build": "vite build && electron-builder --config electron-builder.yml",
    "electron:build:mac": "vite build && electron-builder --config electron-builder.yml --mac",
    "electron:build:mac:all": "vite build && electron-builder --config electron-builder.yml --mac --arm64 --x64",
    "electron:build:mac:arm64": "vite build && electron-builder --config electron-builder.yml --mac --arm64",
    "electron:build:mac:x64": "vite build && electron-builder --config electron-builder.yml --mac --x64",
    "electron:build:win": "vite build && electron-builder --config electron-builder.yml --win",
    "electron:build:win:msix": "vite build && electron-builder --config electron-builder.yml --win",
    "electron:build:win:nsis": "vite build && electron-builder --config electron-builder.yml --win",
    "electron:build:linux": "vite build && electron-builder --config electron-builder.yml --linux",
    "electron:publish": "vite build && electron-builder --config electron-builder.yml --publish always",
    "electron:publish:win": "vite build && electron-builder --config electron-builder.yml --win --publish always",
    "electron:publish:win:nsis": "vite build && electron-builder --config electron-builder.yml --win --publish always",
    "electron:build:win:msix-store": "vite build && electron-builder --config electron-builder.yml --win --x64 --publish never",
    "electron:publish:linux": "vite build && electron-builder --config electron-builder.yml --linux --publish always",
    "electron:publish:mac": "vite build && electron-builder --config electron-builder.yml --mac --publish always",
    "electron:publish:mac:all": "vite build && electron-builder --config electron-builder.yml --mac --arm64 --x64 --publish always",
    "test": "npm rebuild better-sqlite3 && vitest run",
    "test:watch": "vitest",
    "test:main": "vitest run --config vitest.config.main.ts",
    "test:renderer": "vitest run --config vitest.config.renderer.ts",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "media",
    "photo",
    "video",
    "dam",
    "culling"
  ],
  "author": "Zona21 Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0 <22.0.0"
  },
  "overrides": {
    "tar": "^7.5.3",
    "esbuild": "^0.25.0",
    "cacache": "^18.0.4",
    "@xenova/transformers": {
      "onnxruntime-node": "$onnxruntime-node",
      "sharp": "$sharp"
    }
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@sentry/electron": "^7.6.0",
    "@tanstack/react-query": "^5.17.19",
    "@types/canvas-confetti": "^1.9.0",
    "@xenova/transformers": "^2.17.2",
    "archiver": "^7.0.1",
    "better-sqlite3": "^12.6.2",
    "canvas-confetti": "^1.9.4",
    "chokidar": "^3.5.3",
    "date-fns": "^3.0.6",
    "electron-updater": "^6.3.9",
    "exiftool-vendored": "^25.1.0",
    "fast-xml-parser": "^4.3.3",
    "fluent-ffmpeg": "^2.1.2",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.563.0",
    "onnxruntime-node": "^1.23.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-window": "^1.8.10",
    "sharp": "^0.33.1",
    "uuid": "^9.0.1",
    "xxhash-wasm": "^1.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/archiver": "^6.0.2",
    "@types/better-sqlite3": "^7.6.8",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.10",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^33.4.5",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.33",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.3.3",
    "vite": "^6.2.0",
    "vite-plugin-electron": "^0.28.2",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^4.0.17",
    "wait-on": "^7.2.0"
  }
}
